{
    "name": "openclassrooms/akismet-bundle",
    "description": "Akismet Bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "arnaud lefevre",
            "email": "arnaud.lefevre@openclassrooms.com"
        },
        {
            "name": "OpenClassrooms",
            "email": "tech@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenClassrooms\\Bundle\\AkismetBundle\\": ""
        }
    },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\AkismetBundle\\Tests\\": "tests/"
    }
  },
    "require": {
        "php": ">=5.4",
        "symfony/http-kernel": ">=2.7 <4",
        "symfony/dependency-injection": ">=2.7 <4",
        "symfony/config": ">=2.7 <4",
      "openclassrooms/akismet": "dev-fix_sensio_labs_insight_violations"
    },
    "require-dev": {
      "phpunit/phpunit": "~5.5",
        "satooshi/php-coveralls": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
